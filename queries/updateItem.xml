<?xml version="1.0" encoding="utf-8"?>
<query id="updateItem" action="update">
	<tables>
		<table name="module_example_items" />
	</tables>
	<columns>
		<column name="title" var="title" notnull="notnull" />
		<column name="content" var="content" />
		<column name="last_update" var="last_update" />
	</columns>
	<conditions>
		<condition operation="equal" column="item_srl" var="item_srl" filter="number" notnull="notnull" />
	</conditions>
</query>
